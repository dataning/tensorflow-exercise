# installing nbextensions 

 - https://github.com/ipython-contrib/IPython-notebook-extensions

# when you encounter error messages on http://your_jupyter_notebook_url/extensions/ 

 - https://github.com/jupyter-incubator/dashboards

 - jupyter nbextension enable jupyter_dashboards --py --sys-prefix



# installing node.js

source activate tensorflow

sudo apt-get install -y nodejs npm

sudo npm install -g debug

pip install jupyter_dashboards
pip install jupyter_kernel_gateway




#starting jupyter kernel gateway and dashboard server

jupyter kernelgateway --KernelGatewayApp.ip='*'

jupyter-dashboards-server --KERNEL_GATEWAY_URL=http://localhost/


jupyter dashboards quick-setup --sys-prefix

#connecting to jupyter dashboard server 
 http://ip:3000
 
 
 #summarizing default ports
 
 jupyter gateway : 8888
 jupyter dashboard : 3000
 
